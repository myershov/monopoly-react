ul
  list-style-type none
  margin 0
  padding 0
  overflow hidden
  background-color lightgray


li
  float right


li a
  display block
  color #4F4F4F
  text-align center
  padding 14px 16px
  text-decoration none


.active
  color #4682B4


.delphic
  position absolute
  left 70px
  margin-top 12px
  font-size 20px
  font-weight bold
  font-family monospace
  color #4F4F4F
