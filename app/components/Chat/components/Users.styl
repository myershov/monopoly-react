.users-main
  width 22%
  height 100%
  background-color #e6e6e6
  float right
  position absolute
  top 82px
  right 0px
  border-left 1px solid #333
  .users-header
    display flex
    height 60px
    flex-direction row
    border-bottom solid 1px #333
    border-top solid 1px #333
    .my-icon
      height 45px
      width 45px
      border-radius 50%
      background-color #333
      margin-top 7px
      margin-left 10px
    .text
      margin-top 18px
      padding-left 12px
      font-family Helvetica Neue, Helvetica Arial, sans-serif
      font-size 15px
      color black
   .users-body
     display flex
     flex-direction row
     .counter
       font-family Helvetica Neue, Helvetica Arial, sans-serif
       color #333
       margin-left 10px
       margin-top 7px
       font-size 15px
     ul
       list-style-type none
       padding 0px
       width 283px
     li
       height 45px
       width 45px
       border-radius 50%
       background-color #333
       margin-left 10px
       margin-top 7px
       margin-bottom 7px
     a
       display inline-flex
       color black
       padding-left 59px
       margin-top 12px
       margin-bottom 12px
       white-space nowrap
       font-family Helvetica Neue, Helvetica Arial, sans-serif
       font-size 15px
