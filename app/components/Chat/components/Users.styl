.users-main
  width 22%
  background-color #ffffff
  float right
  position absolute
  top 114px
  right 0px
  border-left 1px solid #DBDEE3
  .users-header
    display flex
    height 70px
    flex-direction row
    border-bottom solid 1px #DBDEE3
    border-top solid 1px #DBDEE3
    .my-icon
      margin 15px 10px
    .text
      margin 14px 0px
      padding 5px 0px
      font-family Helvetica Neue, Helvetica Arial, sans-serif
      font-size 15px
      color #22689f
   .users-body
     display flex
     flex-direction row
     overflow auto
     height 477px
     .counter
       font-family Helvetica Neue, Helvetica Arial, sans-serif
       color #333
       margin 11px 10px
       font-size 15px
       margin-bottom 10px
     ul
       list-style-type none
       padding 0px
       width 283px
     li
       margin-left 10px
       margin-top 7px
       display flex
       margin-bottom 7px
     a
       display inline-flex
       color #22689f
       margin 6px 15px
       white-space nowrap
       font-family Helvetica Neue, Helvetica Arial, sans-serif
       font-size 15px
